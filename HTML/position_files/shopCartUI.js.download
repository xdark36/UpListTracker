                        || (fieldname == 'AHOPTSKU')
                        || (fieldname == 'AHRELSKU')
                        || (fieldname == 'AHSLSQTY')
                        || (fieldname == 'AHEPPPRC')
                        || (fieldname == 'AHFABPRC')
                        || (fieldname == 'AHEPPSEL')
                        || (fieldname == 'AHFABSEL')) {
                        i++;
                    }
                }
                if (i >= 7) {
                    return true;
                }
            }
            return false
        },

