        GetFrameSizeCustomOrder: function (size, arms) {
            if (typeof arms == 'undefined') {
                return size;
            }
            var sizeShown = '';
            //Todo: Should change this once we know more about how to do this
            if (size >= 0 && arms) {
                var armValues = {
                    'bailey': ['20', '21', '22'],
                    'track': ['29', '30', '31'],
                    'avery': ['38', '39', '40'],
                };

                if (arms == 'chair') {
                    sizeShown = '30';
                } else {
                    sizeShown = armValues[arms][size];
                }

            }

            return sizeShown;
        },

